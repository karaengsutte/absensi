    <icon density="hdpi" src="./res/icon/android/mipmap-xxxhdpi/eabsensimipa_launcher.png" />
    <icon density="xhdpi" src="./res/icon/android/mipmap-xxxhdpi/eabsensimipa_launcher.png" />
    <icon density="xxhdpi" src="./res/icon/android/mipmap-xxxhdpi/eabsensimipa_launcher.png" />
    <icon density="xxxhdpi" src="./res/icon/android/mipmap-xxxhdpi/eabsensimipa_launcher.png" />
